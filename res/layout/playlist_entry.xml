<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content">
	<ImageView android:id="@+id/EntryIcon" android:layout_width="wrap_content"
		android:src="@android:drawable/ic_media_play" android:paddingLeft="2sp"
		android:paddingRight="2sp" android:layout_height="match_parent"
		android:visibility="gone" />
	<ProgressBar android:paddingLeft="2sp" android:paddingRight="2sp"
		android:layout_width="25sp" android:layout_height="25sp"
		android:id="@+id/progressBarEntry" android:visibility="gone"/>
	<TextView android:id="@+id/entryTitle" android:layout_width="fill_parent"
		android:layout_height="wrap_content" style="@style/PlaylistTheme"
		android:text="Sample" android:gravity="left" android:paddingLeft="2sp"
		android:layout_weight=".5"/>
	<ImageView android:id="@+id/InQueueIcon" android:layout_width="wrap_content"
		android:src="@drawable/greentick" android:paddingLeft="2sp"
		android:paddingRight="2sp" android:layout_height="wrap_content"/>
		<!-- android:visibility="gone"  --> 
</LinearLayout>
